	<versionNumber>2.0.0</versionNumber>
	<platforms>

		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>com.jetbrains.kotlin.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.kotlin.KotlinExtension</initializer>
				<finalizer>com.distriqt.extension.kotlin.KotlinExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-ARM64">
			<applicationDeployment>
				<nativeLibrary>com.jetbrains.kotlin.android.jar</nativeLibrary>
