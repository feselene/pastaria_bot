<extension xmlns="http://ns.adobe.com/air/extension/50.2">
	<id>com.distriqt.InAppBilling</id>
	<versionNumber>16.1.2</versionNumber>
	<platforms>

		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>com.distriqt.inappbilling.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.inappbilling.InAppBillingExtension</initializer>
				<finalizer>com.distriqt.extension.inappbilling.InAppBillingExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-ARM64">
			<applicationDeployment>
				<nativeLibrary>com.distriqt.inappbilling.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.inappbilling.InAppBillingExtension</initializer>
				<finalizer>com.distriqt.extension.inappbilling.InAppBillingExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>com.distriqt.inappbilling.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.inappbilling.InAppBillingExtension</initializer>
				<finalizer>com.distriqt.extension.inappbilling.InAppBillingExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform name="Android-x64">
			<applicationDeployment>
				<nativeLibrary>com.distriqt.inappbilling.android.jar</nativeLibrary>
				<initializer>com.distriqt.extension.inappbilling.InAppBillingExtension</initializer>
				<finalizer>com.distriqt.extension.inappbilling.InAppBillingExtension</finalizer>
			</applicationDeployment>
		</platform>

		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libInAppBilling.a</nativeLibrary>
				<initializer>InAppBillingExtInitializer</initializer>
				<finalizer>InAppBillingExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
